#!/bin/bash

ROOT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"
pushd $ROOT_DIR
docker run \
  --name postgresql \
  -e POSTGRES_DB=ecodatum \
  -e POSTGRES_USER=ecodatum \
  -e POSTGRES_PASSWORD=password \
  -p 5432:5432 \
  -d \
  mdillon/postgis:10 #\
#  -c max_connections=200 \
#  -c log_statement='all' \
#  -c log_error_verbosity='VERBOSE'
popd
